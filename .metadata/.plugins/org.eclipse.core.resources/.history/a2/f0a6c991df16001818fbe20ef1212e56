package com.mysecurity.core.validate.code.sms;

/**
 * @author Liziyue
 *
 */
public class DefaultSmsCodeSender implements SmsCodeSender {

	/* (non-Javadoc)
	 * @see com.mysecurity.core.validate.code.sms.SmsCodeSender#send(java.lang.String, java.lang.String)
	 */
	//默认短信发送
	@Override
	public void send(String mobile, String code) {
		System.out.println("向手机发送"+mobile+"短信验证码"+code);
	}

}
